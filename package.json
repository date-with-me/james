    "build": "esbuild src/index.tsx --bundle --outfile=dist/index.js --minify && tailwindcss -i ./src/index.css -o ./dist/index.css --minify",
    "build:github": "node tools/build.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "repl": "npx ts-node"
  },
